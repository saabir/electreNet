<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ch.albertlegrand.data.model.Booktitle" table="Booktitle" catalog="meschema">
        <id name="booktitleid">
            <column name="booktitleid" sql-type="bigint unsigned" length="20" not-null="true"/>
        </id>
        <property name="version">
            <column name="version" sql-type="bigint" length="19" not-null="true"/>
        </property>
        <property name="diliCwordingExtended">
            <column name="diliCwordingExtended" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="diliCwordingStandard">
            <column name="diliCwordingStandard" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="diliCwordingCounter">
            <column name="diliCwordingCounter" sql-type="varchar" length="20"/>
        </property>
        <property name="eleNetTitle">
            <column name="eleNetTitle" sql-type="varchar" length="100"/>
        </property>
        <property name="eleNetCompleteTitle">
            <column name="eleNetCompleteTitle" sql-type="varchar" length="150"/>
        </property>
        <property name="wCatBtitle">
            <column name="wCatBtitle" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>